<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ParcelRegistry.Importer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>

  <connectionStrings>
    <add name="CRABEntities" connectionString="metadata=res://*/Entity.CRAB.csdl|res://*/Entity.CRAB.ssdl|res://*/Entity.CRAB.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=CRAB;integrated security=True;MultipleActiveResultSets=True;App=Vbr.CrabRemoteRepo&quot;" providerName="System.Data.EntityClient"/>
  </connectionStrings>

  <applicationSettings>
    <ParcelRegistry.Importer.Properties.Settings>
      <setting name="BatchSize" serializeAs="String">
        <value>1000</value>
      </setting>
      <setting name="HttpTimeoutInMinutes" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="LogFileSizeLimitBytes" serializeAs="String">
        <value>104857600</value>
      </setting>
      <setting name="ImportEndpoint" serializeAs="String">
        <value>v1/crabimport</value>
      </setting>
      <setting name="NrOfProducers" serializeAs="String">
        <value>10</value>
      </setting>
      <setting name="NrOfConsumers" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="TimeMarginBeforeNowInSeconds" serializeAs="String">
        <value>00:03:00</value>
      </setting>
      <setting name="BaseUrl" serializeAs="String">
        <value>http://localhost:7000</value>
      </setting>
      <setting name="ImportAuthPass" serializeAs="String">
        <value />
      </setting>
      <setting name="BufferSize" serializeAs="String">
        <value>2000</value>
      </setting>
      <setting name="ImportAuthUser" serializeAs="String">
        <value />
      </setting>
      <setting name="ImportBatchStatusEndpoint" serializeAs="String">
        <value>v1/crabimport/batch</value>
      </setting>
      <setting name="WaitForUserInput" serializeAs="String">
        <value>True</value>
      </setting>
    </ParcelRegistry.Importer.Properties.Settings>
  </applicationSettings>
</configuration>
